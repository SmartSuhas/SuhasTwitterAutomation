package Locators;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class SearchUserOrTermLocators {
	
	static WebElement element = null; 
	
	public static WebElement ExploreTabClick(WebDriver driver) {
		element =  driver.findElement(By.xpath("//*[@id=\"react-root\"]/div/div/div[2]/header/div/div/div/div[1]/div[2]/nav/a[2]/div"));
		return element;
	}
	
	public static WebElement EnterNameOrTerm(WebDriver driver) {
		element = driver.findElement(By.xpath("//*[@id=\"react-root\"]/div/div/div[2]/main/div/div/div/div/div/div[1]/div[1]/div/div/div/div/div[1]/div[2]/div/div/div/form/div[1]/div/div/label/div[2]/div/input"));
		return element;
	}
	
	public static WebElement ClickEnterButton(WebDriver driver) {
		element =  driver.findElement(By.xpath("//*[@id=\"react-root\"]/div/div/div[2]/main/div/div/div/div/div/div[1]/div[1]/div/div/div/div/div[1]/div[2]/div/div/div/form/div[1]/div/div/label/div[2]/div/input"));
		return element;
	}
}
